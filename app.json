{
  "expo": {
    "owner": "secondvision",
    "name": "Second Vision",
    "slug": "secondvision",
    "sdkVersion": "54.0.0",
    "version": "1.0.5",
    "orientation": "portrait",
    "icon": "./src/shared/assets/images/logo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "cli": {
      "appVersionSource": "remote"
    },
    "build": {
      "production": {
        "releaseChannel": "production",
        "distribution": "store"
      }
    },
    "android": {
      "versionCode": 5,
      "adaptiveIcon": {
        "foregroundImage": "./src/shared/assets/images/build/ic_launcher_foreground.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.BLUETOOTH",
        "android.permission.BLUETOOTH_ADMIN",
        "android.permission.BLUETOOTH_CONNECT"
      ],
      "package": "com.rotech.ble_connection"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./src/shared/assets/images/build/ic_launcher_foreground.png"
    },
    "plugins": [
      [
        "react-native-ble-plx",
        {
          "isBackgroundEnabled": true,
          "modes": [
            "peripheral",
            "central"
          ],
          "bluetoothAlwaysPermission": "Allow $(PRODUCT_NAME) to connect to bluetooth devices"
        }
      ],
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "4ea6acdf-bbea-486d-b12d-bac3d85a5134"
      }
    },
    "ios": {
      "bundleIdentifier": "com.rotech.ble-connection"
    }
  }
}
